{
  "rewrites": [
    {
      "source": "/",
      "destination": "/web/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/web/$1"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "nano-banana.live"
        }
      ],
      "destination": "https://www.nano-banana.live/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "nano-banana.online"
        }
      ],
      "destination": "https://www.nano-banana.live/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.nano-banana.online"
        }
      ],
      "destination": "https://www.nano-banana.live/$1",
      "permanent": true
    }
  ]
}